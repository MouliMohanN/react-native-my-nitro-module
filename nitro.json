{
  "$schema": "https://nitro.margelo.com/nitro.schema.json",
  "cxxNamespace": [
    "mynitromodule"
  ],
  "ios": {
    "iosModuleName": "MyNitroModule"
  },
  "android": {
    "androidNamespace": [
      "mynitromodule"
    ],
    "androidCxxLibName": "MyNitroModule"
  },
  "autolinking": {
    "MyNitroModule": {
      "swift": "HybridMyNitroModule",
      "kotlin": "HybridMyNitroModule"
    }
  },
  "ignorePaths": [
    "**/node_modules"
  ]
}